{
    "comments": {
        "lineComment": "#"
    },
    "surroundingPairs": [
        ["'", "'"],
        ["\"", "\""],
        [ "(", ")" ],
        [ "[", "]" ],
    ],
    "indentationRules": {
        "increaseIndentPattern": "^\\s*(if|choose|meta|command|\\*)(\\s|$)",
        "decreaseIndentPattern": "^\\s*(end|\\*)"
    }
}